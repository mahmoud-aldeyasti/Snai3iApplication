@{
}
@using Snai3i_BLL.DTO.ReviewDto

@model IEnumerable<ReviewReadDTO>

<h2>Tools List</h2>
<table class="table">
    <thead>
        <tr>
            <th>UserId</th>
            <th>WorkerId</th>
            <th>
                Comment
            </th>
            <th>
                Rate
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var tool in Model)
        {
            <tr>
                <td>@tool.UserName</td>
                <td>@tool.workername</td>
                <td>@tool.Comment</td>
                <td>@tool.Rate</td>



            </tr>
        }
    </tbody>
</table>
